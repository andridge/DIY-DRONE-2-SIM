<!-- Provide a Positive Velocity -->
	<plugin name="quad_prop4" filename="libLiftDragPlugin.dylib">
	<a0>0.1</a0>
	<cla>0.1</cla>
	<cda>0.01</cda>
	<cma>0.0</cma>
	<alpha_stall>0.2</alpha_stall>
