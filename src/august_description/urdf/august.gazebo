	<cp>0 0.5 0</cp>
	<area>0.762</area>
	<air_density>1.2041</air_density>
	<forward>1 0 0</forward>
	<upward>0 0 1</upward>
	<link_name>a_1_1</link_name>
	<control_joint>Revolute 55</control_joint>
	</plugin>
</gazebo>
<gazebo>
<!-- Provide a Negative Velocity -->
	<plugin name="quad_prop3" filename="libLiftDragPlugin.dylib">
	<a0>0.1</a0>
	<cla>0.1</cla>
